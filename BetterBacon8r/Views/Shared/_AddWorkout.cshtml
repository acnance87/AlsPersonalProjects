@model IEnumerable<WorkoutTypes>

@foreach (var exercise in Model) {
    <input type="checkbox" name="workoutsSelected" class="workoutSelect" id="@($"{exercise.WorkoutTypeID}-{exercise.ExerciseName}")" />
    <label for="@($"{exercise.WorkoutTypeID}-{exercise.ExerciseName}")">@exercise.ExerciseName</label>
    <br />
}
